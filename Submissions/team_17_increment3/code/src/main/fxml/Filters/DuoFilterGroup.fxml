<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<AnchorPane prefWidth="600.0" stylesheets="@../../css/Global.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uk.ac.soton.git.comp2211g17.viewmodel.filters.DuoFilterGroupViewModel"
            styleClass="print-hidden">
    <children>
        <HBox prefHeight="100.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
            <BorderPane HBox.hgrow="ALWAYS">
               <center>
                      <Button fx:id="dateButton" onAction="#selectTab" styleClass="blueButton" text="Date" />
               </center>
            </BorderPane>
            <BorderPane HBox.hgrow="ALWAYS">
               <center>
                      <Button fx:id="audienceButton" onAction="#selectTab" styleClass="whiteButton" text="Audience" />
               </center>
            </BorderPane>
            <BorderPane HBox.hgrow="ALWAYS">
                <center>
                    <Button fx:id="contextButton" onAction="#selectTab" styleClass="whiteButton" text="Context" />
                </center>
            </BorderPane>
            </children>
        </HBox>
        <HBox AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
            <BorderPane fx:id="containerLeft" prefWidth="600.0" />
            <BorderPane fx:id="containerRight" prefWidth="600.0" />
        </HBox>
        <VBox alignment="CENTER_RIGHT" AnchorPane.bottomAnchor="7.0" AnchorPane.leftAnchor="7.0" AnchorPane.rightAnchor="7.0" AnchorPane.topAnchor="315.0">
         <children>
             <Button mnemonicParsing="false" styleClass="blueButton" text="Filter" fx:id="filterButton">
                 <graphic>
                     <FontIcon iconLiteral="eva-funnel-outline" styleClass="textBlue" />
                 </graphic>
             </Button>
         </children></VBox>
    </children>
</AnchorPane>
